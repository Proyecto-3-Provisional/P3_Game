material bubble {
	technique {
		pass {
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse vertexcolour
			texture_unit {
				texture bubble.png
				tex_address_mode clamp
				colour_op modulate
			}
		}
	}
}

material bubble_blue {
	technique {
		pass {
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse vertexcolour
			texture_unit {
				texture bubble_blue.png
				tex_address_mode clamp
				colour_op modulate
			}
		}
	}
}